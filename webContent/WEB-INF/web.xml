<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  
  <display-name>Online_Transport_System</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
 
 <servlet>
 	<servlet-name>CustomerSignUp</servlet-name>
 	<servlet-class>com.Customer.CustometInsert</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>CustomerSignUp</servlet-name>
 	<url-pattern>/insert</url-pattern>
 </servlet-mapping>

 <servlet>
	<servlet-name>CustomerLogin</servlet-name>
	<servlet-class>com.Customer.LoginServlet</servlet-class>
 </servlet>
 <servlet-mapping>
	<servlet-name>CustomerLogin</servlet-name>
	<url-pattern>/log</url-pattern>
 </servlet-mapping>
 
 <servlet>
 	<servlet-name>CustomerUpdate</servlet-name>
 	<servlet-class>com.Customer.CustomerUpdateServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>CustomerUpdate</servlet-name>
 	<url-pattern>/update</url-pattern>
 </servlet-mapping>
 
 <servlet>
 	<servlet-name>SheduleATrip</servlet-name>
 	<servlet-class>com.Trip.TripInsert</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>SheduleATrip</servlet-name>
 	<url-pattern>/shedule</url-pattern>
 </servlet-mapping>
 
 <servlet>
 	<servlet-name>CustomerDelete</servlet-name>
 	<servlet-class>com.Customer.CustomerDeleteServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>CustomerDelete</servlet-name>
 	<url-pattern>/delete</url-pattern>
 </servlet-mapping>
 
 <servlet>
    <servlet-name>TripUpdate</servlet-name>
	<servlet-class>com.Trip.TripUpdateServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>TripUpdate</servlet-name>
	<url-pattern>/tupdate</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>TripDelete</servlet-name>
	<servlet-class>com.Trip.TripDeleteServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>TripDelete</servlet-name>
	<url-pattern>/tdelete</url-pattern>
</servlet-mapping>

 </web-app>